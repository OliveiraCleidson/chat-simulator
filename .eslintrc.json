{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error",
        "no-undef": "off"
    }
}
